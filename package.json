{
  "name": "plutus-fe",
  "version": "1.0.0",
  "description": "Personal finance tracker",
  "main": "index.js",
  "repository": "https://github.com/mashafrancis/plutus-fe.git",
  "author": "Francis Masha Konde <francismasha96@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "NODE_OPTIONS='--max_old_space_size=4096' next build",
    "start": "next start",
    "prettier:fix": "prettier './src/**/*.{tsx,ts}' --write",
    "eslint:fix": "eslint ./src --fix",
    "codeStyle:fix": "yarn prettier:fix && yarn eslint:fix",
    "husky": "pretty-quick --staged && yarn prettier && yarn tslint",
    "prettier": "prettier --write './src/**/*.{tsx,ts}'",
    "tslint": "tsc --noEmit && eslint \"*/**/*.{tsx,ts}\" --quiet",
    "test:unit": "cross-env NODE_ENV=testing jest -u --maxWorkers=4",
    "test:coverage": "yarn test:unit --coverage",
    "test:watch": "yarn test:unit --watch --verbose",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "prepare": "husky install",
    "commit": "cz",
    "commit:signed": "cz -S",
    "clear-all": "rimraf ./.cache ./.next ./dist"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.73",
    "@mui/material": "^5.5.1",
    "@mui/styles": "^5.5.1",
    "@mui/x-data-grid": "^5.6.1",
    "@reduxjs/toolkit": "^1.8.0",
    "aos": "^2.3.4",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.0",
    "dotenv": "^16.0.0",
    "jarallax": "^2.0.2",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "next": "^12.1.0",
    "next-auth": "^4.3.1",
    "next-images": "^1.8.4",
    "next-optimized-images": "^2.6.2",
    "next-pwa": "^5.4.6",
    "next-redux-wrapper": "^7.0.5",
    "nextjs-cors": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazy-load-image-component": "^1.5.1",
    "react-redux": "^7.2.6",
    "validate.js": "^0.13.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/aos": "^3.0.4",
    "@types/cookie": "^0.4.1",
    "@types/file-saver": "^2.0.5",
    "@types/http-proxy": "^1.17.8",
    "@types/js-cookie": "^3",
    "@types/node": "^17.0.21",
    "@types/react": "^17",
    "@types/react-dom": "^17.0.13",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-redux": "^7.1.23",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/yup": "^0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.2",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.6",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.2",
    "prettier": "^2.6.0",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.6.2"
  }
}
